{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "ConnectionString": "Host=localhost;Database=agentchat;Username=postgres;Password=postgres",
    "MaxRetryCount": 3,
    "CommandTimeout": 30
  },
  "RabbitMq": {
    "HostName": "localhost",
    "Port": 5672,
    "UserName": "guest",
    "Password": "guest",
    "VirtualHost": "/",
    "PrefetchCount": 10
  },
  "Redis": {
    "ConnectionString": "localhost:6379",
    "Database": 0,
    "InstanceName": "agent-chat:"
  },
  "Auth": {
    "Authority": "https://your-identity-provider.com",
    "Audience": "agent-chat-api"
  },
  "AzureOpenAi": {
    "Endpoint": "",
    "ApiKey": "",
    "DefaultDeployment": "gpt-4o"
  },
  "Telemetry": {
    "ServiceName": "agent-chat-api-bff",
    "OtlpEndpoint": "",
    "EnableConsoleExporter": true
  },
  "Cors": {
    "Origins": ["http://localhost:5173", "http://localhost:5174", "http://localhost:3000"]
  }
